

CREATE COMPUTE MODULE APIC_TOKEN_CALL_USING_UDP_MSG_FLOW_Compute1
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
		DECLARE TOKEN CHARACTER InputRoot.JSON.Data.access_token;
		DECLARE BEARER CHARACTER 'Bearer ';
		SET OutputRoot.HTTPRequestHeader.Authorization = (BEARER || TOKEN);
		RETURN TRUE;
	END;
END MODULE;
