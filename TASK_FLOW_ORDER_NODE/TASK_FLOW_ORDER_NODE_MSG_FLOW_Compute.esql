

CREATE COMPUTE MODULE TASK_FLOW_ORDER_NODE_MSG_FLOW_Compute
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
		DECLARE INREF REFERENCE TO InputRoot.XMLNSC.Customer;
		IF FIELDNAME(INREF.*[<]) <> 'Address' THEN
			THROW USER EXCEPTION;
		ELSE 
			SET INREF.PersonalInfo.StatusCode = 0;
		END IF;
	END;
END MODULE;
